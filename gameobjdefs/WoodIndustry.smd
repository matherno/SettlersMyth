<gameobjectdef name="Wood Cutter" type="Harvester">
    <grid x="3" y="2">
        <output x="0" y="1"/>
        <!-- <lotentry x="1" y="1" xdir="0" ydir="-1"/> same as building entry if not defined -->
        <buildingentry x="1" y="1" xdir="0" ydir="-1"/>
    </grid>
    <harvester>
        <deposit name="Tree" harvesttime="2000"/>  <!-- time in ms for gatherering 1 resource worth -->
    </harvester>
    <render>
        <meshfile>buildings/Woodcutter.obj</meshfile>
    </render>
    <icon imagefile="icons/woodcutter.png"/>
    <construction time="10000">
    	<resource name="Log" amount="4"/>
    	<resource name="Thatch" amount="2"/>
    </construction>
</gameobjectdef>



<gameobjectdef name="Sawmill" type="Manufacturer">
    <grid x="3" y="2">
        <output x="0" y="1"/>
        <buildingentry x="1" y="1" xdir="0" ydir="-1"/>
    </grid>
    <manufacturer process_time="5000">
        <manufacturespot x="0" y="1"/> 
        <input name="Log" amount="1"/> 
        <output name="Plank" amount="1"/> 
    </manufacturer>
    <render>
        <meshfile>buildings/Sawmill.obj</meshfile>
    </render>
    <icon imagefile="icons/sawmill.png"/>
    <construction time="10000">
        <resource name="Log" amount="4"/>
        <resource name="Thatch" amount="2"/>
    </construction>
</gameobjectdef>

