<blueprint name="Tree" type="Deposit" buildable="true">
    <icon imagefile="icons/tree.png"/>
    <grid x="1" y="1" height="2.0" posincentre="true"/>
    <deposit resource_name="Log" resource_amount="4"/>
    <component type="VoxelRenderable" id="1" staticmodel="true">
        <model voxelfile="deposits/TreeA.mgv"/>
        <model voxelfile="deposits/TreeB.mgv"/>
        <model voxelfile="deposits/TreeC.mgv"/>
    </component>
    <component type="DestroyWhenNoResource" id="2"/>    
    <component type="ResListHUD" id="3">
        <resource name="Log"/>
    </component>
</blueprint>

