<gameobjectdef name="Wood Cutter" type="Harvester">
    <grid x="4" y="4">
        <resourcestack x="3" y="2"/>
        <entry x="2" y="3"/>
    </grid>
    <harvester>
        <deposit name="Tree" harvesttime="2000"/>
    </harvester>
    <render>
        <renderpart voxelfile="buildings/Woodcutter.mgv"/>
    </render>
    <icon imagefile="icons/woodcutter.png"/>
    <construction time="10000">
    	<resource name="Log" amount="4"/>
    	<resource name="Thatch" amount="2"/>
    </construction>
</gameobjectdef>


<gameobjectdef name="Sawmill" type="Manufacturer">
    <grid x="4" y="4">
        <resourcestack x="3" y="3"/>
        <resourcestack x="2" y="3"/>
        <entry x="1" y="3"/>
    </grid>
    <manufacturer process_time="5000">
        <manufacturespot x="2" y="2"/> 
        <manufacturedir x="1" y="0"/> 
        <input name="Log" amount="1"/> 
        <output name="Plank" amount="1"/> 
    </manufacturer>
    <render>
        <renderpart voxelfile="buildings/Sawmill.mgv"/>
    </render>
    <icon imagefile="icons/sawmill.png"/>
    <construction time="10000">
        <resource name="Log" amount="4"/>
        <resource name="Thatch" amount="2"/>
    </construction>
</gameobjectdef>


